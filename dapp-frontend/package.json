{
  "name": "k-swap",
  "version": "1.0.0",
  "description": "Fully on-chain AMM-style DEX for Keeta Network",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "NODE_ENV=test jest",
    "test:unit": "NODE_ENV=test jest --testNamePattern=\"Mathematical Functions|Error Handling\"",
    "test:integration": "NODE_ENV=test jest --testNamePattern=\"Integration Tests\"",
    "test:testnet": "NODE_ENV=test NEXT_PUBLIC_NETWORK=testnet jest src/tests/kswap-integration.test.ts",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "cli": "ts-node src/cli/kswap-cli.ts",
    "cli:demo": "ts-node src/cli/kswap-cli.ts demo",
    "cli:init": "ts-node src/cli/kswap-cli.ts init",
    "testnet:setup": "ts-node scripts/testnet-setup.ts full",
    "testnet:init": "ts-node scripts/testnet-setup.ts init", 
    "testnet:tokens": "ts-node scripts/testnet-setup.ts tokens",
    "testnet:diag": "node scripts/testnet-setup.js",
    "validate": "npm run type-check && npm run lint && npm run test:unit",
    "validate:full": "npm run validate && npm run test:integration"
  },
  "dependencies": {
    "@keetanetwork/keetanet-client": "latest",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.292.0",
    "next": "^14.0.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.3.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.0"
  },
  "keywords": [
    "defi",
    "dex",
    "amm",
    "keeta",
    "blockchain",
    "swap"
  ],
  "author": "K-Swap Team",
  "license": "SEE LICENSE IN LICENSE"
}
